{
  "name": "intro-section-with-dropdown-navigation-main",
  "version": "1.0.0",
  "description": "This is a solution to the [Intro section with dropdown navigation challenge on Frontend Mentor](https://www.frontendmentor.io/challenges/intro-section-with-dropdown-navigation-ryaPetHE5). Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlegSyng/intro-section-with-dropdown-navigation-main.git"
  },
  "keywords": [],
  "author": "OlegSyng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OlegSyng/intro-section-with-dropdown-navigation-main/issues"
  },
  "homepage": "https://github.com/OlegSyng/intro-section-with-dropdown-navigation-main#readme",
  "devDependencies": {
    "live-server": "^1.2.2",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5"
  }
}
